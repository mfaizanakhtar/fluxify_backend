{
  "name": "esim_backend",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "dev:worker": "ts-node-dev --respawn --transpile-only src/worker/index.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:worker\"",
    "ngrok": "./scripts/ngrok.sh",
    "webhook:register": "ts-node scripts/register-http-webhook.ts",
    "webhook:list": "ts-node scripts/list-webhooks.ts",
    "webhook:delete": "ts-node scripts/delete-webhook.ts",
    "webhook:update": "ts-node scripts/update-webhook.ts",
    "webhook:check-scopes": "ts-node scripts/check-scopes.ts",
    "fetch:skus": "ts-node scripts/fetch-firoam-skus.ts",
    "find:priceid": "ts-node scripts/find-package-priceid.ts",
    "db:seed": "ts-node prisma/seed-sku-mappings.ts",
    "init:railway": "ts-node scripts/init-railway.ts",
    "generate:skus": "ts-node scripts/generate-shopify-skus.ts",
    "match:firoam": "ts-node scripts/match-firoam-packages.ts",
    "generate:seed": "ts-node scripts/generate-seed-script.ts",
    "seed:all-mappings": "ts-node prisma/seed-all-mappings.ts",
    "verify:mappings": "ts-node scripts/verify-mappings.ts",
    "update:export": "ts-node scripts/update-shopify-export.ts",
    "analyze:missing": "ts-node scripts/analyze-missing-skus.ts",
    "clean:export": "ts-node scripts/clean-shopify-export.ts",
    "db:clean-orphaned": "ts-node scripts/clean-orphaned-mappings.ts",
    "pipeline": "./scripts/run-pipeline.sh",
    "pipeline:help": "echo '\nðŸ“‹ SKU Pipeline Commands:\n\n  npm run pipeline        - Run full pipeline (all phases)\n  npm run generate:skus   - Phase 1: Generate SKUs\n  npm run match:firoam    - Phase 2: Match with FiRoam\n  npm run generate:seed   - Phase 3: Generate seed script\n  npm run seed:all-mappings - Phase 4: Deploy to database\n  npm run update:export   - Phase 5: Update Shopify export\n  npm run clean:export    - Phase 6: Remove unfulfillable products\n  npm run verify:mappings - Phase 7: Verify mappings\n  npm run analyze:missing - Analyze missing SKUs\n\n'",
    "test:component": "vitest run src/tests/firoam.component.test.ts --reporter verbose",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.ts",
    "worker": "ts-node-dev --respawn --transpile-only src/worker/index.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "test": "vitest"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "@types/crypto-js": "^4.2.2",
    "@types/pdfkit": "^0.17.4",
    "@types/qrcode": "^1.5.6",
    "axios": "^1.5.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.0.0",
    "fastify": "^4.22.0",
    "pdfkit": "^0.17.2",
    "pg": "^8.11.0",
    "pg-boss": "^8.0.0",
    "prisma": "^5.0.0",
    "qrcode": "^1.5.1",
    "resend": "^6.9.1",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "nock": "^13.3.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6",
    "vitest": "^4.0.16"
  }
}
